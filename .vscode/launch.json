{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run StarterKit-HTML5Module",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/starterkit-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/nsHTML5Module/index.html?sap-ui-language=EN",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/HTML5Module",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/StarterKit/HTML5Module\"}"
      },
      "envFile": "${workspaceFolder}/HTML5Module/.env1",
      "preLaunchTask": "Build StarterKit"
    },
    {
      "name": "Run Mock StarterKit-HTML5Module",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/starterkit-approuter/node_modules/@sap/html5-repo-mock/index.js",
